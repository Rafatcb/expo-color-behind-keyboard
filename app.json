{
  "expo": {
    "name": "Color behind keyboard",
    "slug": "color-behind-keyboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2962ff"
    },
    "updates": {
